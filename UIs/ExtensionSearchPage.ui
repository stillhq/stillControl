<interface>
  <requires lib="gtk" version="4.12"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="ExtensionSearchPage" parent="GtkBox">
    <property name="hexpand">True</property>
    <property name="margin-bottom">20</property>
    <property name="margin-top">20</property>
    <property name="orientation">vertical</property>
    <property name="spacing">10</property>
    <child>
      <object class="GtkLabel">
        <property name="label">Search from extensions.gnome.org</property>
        <style>
          <class name="title-2"/>
        </style>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="hexpand">True</property>
        <property name="spacing">5</property>
        <child>
          <object class="GtkSearchEntry" id="search_entry">
            <property name="hexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkDropDown" id="sort_dropdown">
            <property name="model">
              <object class="GtkStringList">
                <property name="strings">Relevance
Downloads
Recent
Name</property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwPreferencesGroup" id="extensions_group"/>
    </child>
  </template>
</interface>
